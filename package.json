{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "quality": "node scripts/quality-check.js all",
    "quality:dead-code": "node scripts/quality-check.js dead-code",
    "quality:duplicates": "node scripts/quality-check.js duplicates",
    "quality:circular": "node scripts/quality-check.js circular",
    "quality:dead-ui": "node scripts/quality-check.js dead-ui",
    "quality:packages": "node scripts/quality-check.js packages",
    "prepare": "husky",
    "size": "size-limit",
    "size:check": "size-limit"
  },
  "size-limit": [
    {
      "name": "First Load JS (shared)",
      "path": ".next/static/chunks/main-*.js",
      "limit": "100 KB"
    },
    {
      "name": "First Load JS (app)",
      "path": ".next/static/chunks/app/page-*.js",
      "limit": "50 KB"
    }
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.20",
    "@trpc/client": "^11.8.1",
    "@trpc/next": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.563.0",
    "next": "^15.1.0",
    "prisma": "^5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.71.1",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@next/eslint-plugin-next": "^15.1.0",
    "@playwright/test": "^1.49.0",
    "@size-limit/preset-app": "^11.0.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^2.1.0",
    "autoprefixer": "^10",
    "commander": "^14.0.2",
    "eslint": "^9.17.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.0",
    "jscpd": "^4.0.7",
    "jsdom": "^25.0.0",
    "knip": "^5.81.0",
    "lint-staged": "^15.3.0",
    "madge": "^8.0.0",
    "postcss": "^8",
    "prettier": "^3.4.0",
    "size-limit": "^11.0.0",
    "tailwindcss": "^3",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.19.0",
    "vitest": "^2.1.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
